<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="bv_sequence_view" model="ir.ui.view">
        <field name="model">ir.sequence</field>
        <field name="inherit_id" ref="base.sequence_view" />
        <field name="arch" type="xml">
            <xpath expr="//group[2]//group[2]//span[3]" position="after">
                <span colspan="2">Financial Year: %%(fy)s</span>
            </xpath>
        </field>
    </record>
</odoo>